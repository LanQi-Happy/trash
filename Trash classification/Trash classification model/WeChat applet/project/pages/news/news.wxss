.img_list{
	width: 100vw;
	display: flex;
	display: -webkit-flex;
  padding: 0 20rpx;
  box-sizing: border-box;
  flex-wrap: wrap;
  -webkit-flex-wrap:wrap;
}
.img_list .img_li,
.addimg{
	width: 300rpx;
	height: 250rpx;
	border: 1px solid #999999;
  margin: 200rpx;
  flex-shrink: 0;
  -webkit-flex-shrink:0;
}
.addimg image{
	width: 200rpx;
	height: 150rpx;
	margin: 50rpx 60rpx;
}
.img_list .img_li image{
  width: 100%;
  height: 100%;
}
